{
  "name": "Traverse",
  "description": "How many URLs do you visit?",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "tabs", "scripting"],
  "action": {
    "default_popup": "popup_main.html",
    "default_icon": {
      "16": "/images/traverse16.png",
      "32": "/images/traverse32.png",
      "48": "/images/traverse48.png",
      "128": "/images/traverse128.png"
    }
  },
  "icons": {
    "16": "/images/traverse16.png",
    "32": "/images/traverse32.png",
    "48": "/images/traverse48.png",
    "128": "/images/traverse128.png"
  }
}
